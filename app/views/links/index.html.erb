<div class="link-page-wrapper">
    <div class="link-page">
        <div class="link-list">
            <% @links.each do |link| %>
                <div class="card link">
                    <div class="link-describe">
                        <%= link.describe %>
                    </div>

                    <div class="link-content">
                        <%= link_to link.link %>
                    </div>
                </div>
            <% end %>
        </div>
    </div>
</div>
